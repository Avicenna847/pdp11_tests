ostat = 177564
odata = 177566

	.=200
N:      .WORD  5    
STR:	.ASCIZ /Hello, world!/
STR1:	.ASCII /Hello, world!/

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
; R1 - адрес очередного символа
; R0 - значение ascii кода этого символа (число)
; R2 - счетчик оставшегося количества итераций
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

	.=1000
	mov     #STR, R1
    mov     N, R2
LOOP_STR:
	movb    (R1)+, R0
putc:
	tstb	ostat
	bpl	    putc
	movb	R0, odata
	sob     R2, LOOP_STR
END:
	halt
